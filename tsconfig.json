{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "exclude": ["__tests__/**/*-test.ts"],
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["components/*"],
      "@screens/*": ["screens/*"],
      "@utils/*": ["utils/*"],
      "@assets/*": ["assets/*"],
      "@routes/*": ["routes/*"],
      "@constants/*": ["constants/*"],
      "@navigation/*": ["navigation/*"],
      "@services/*": ["services/*"],
      "@store/*": ["store/*"],
      "@modules/*": ["modules/*"],
      "@types/*": ["types/*"],
      "@hooks/*": ["hooks/*"],
      "@theme/*": ["theme/*"],
      "@i18n/*": ["i18n/*"],
      "@config/*": ["config/*"],
      "@api/*": ["api/*"],
      "@models/*": ["models/*"]
    }
  }
}
